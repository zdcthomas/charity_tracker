<!DOCTYPE html>
<html>
  <head>
    <title>CharityTracker</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render partial: "/partials/nav" %>
    <%= render partial: "/partials/auth" unless current_page?(login_path || new_user_path) %>
    <%= yield %>
  </body>
</html>
