
<% if current_user %>
  <li>
    <%= link_to  "My Profile", user_path(current_user)%>
  </li>
  <li>
    <%= link_to  "Log Out", logout_path, data: { confirm:"Are you sure you would like to log out?" } %>
  </li>
<% else %>
  <li>
    <%= link_to  "Sign Up", new_user_path%>
  </li>
  <li>
    <%= link_to  "Login", login_path %>
  </li>
<% end %>
